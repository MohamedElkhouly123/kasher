                    android:visibility="gone"
                    android:layout_marginTop="@dimen/_10sdp"
                    >
                    <androidx.viewpager2.widget.ViewPager2
                        android:id="@+id/view_pager2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintBottom_toTopOf="@+id/guideline"
                        app:layout_constraintDimensionRatio="1:1"
                        />

                    <com.tbuonomo.viewpagerdotsindicator.WormDotsIndicator
                        android:id="@+id/worm_dots_indicator"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:dotsColor="@color/app_color"
                        android:elevation="@dimen/_55sdp"
                        android:layout_marginBottom="@dimen/_4sdp"
                        android:layout_gravity="center|bottom"
                        app:dotsStrokeColor="#B3FFFFFF"
                        app:dotsCornerRadius="@dimen/_6sdp"
                        app:dotsSize="@dimen/_10sdp"
                        app:dotsSpacing="@dimen/_3sdp"
                        android:scrollbarFadeDuration="@integer/material_motion_duration_long_1"
                        app:dotsStrokeWidth="@dimen/_2sdp"
                        />
                </FrameLayout>

                <RelativeLayout
